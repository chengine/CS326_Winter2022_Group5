<?xml version="1.0" ?>
<robot name="yellow_cube">
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.05" />
      <!-- I = m * s^2 / 6 -->
      <inertia  ixx="3.3e-6" ixy="0.0"  ixz="0.0"  iyy="3.3e-6"  iyz="0.0"  izz="3.3e-6" />
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.02 0.02 0.02" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
    <material>Gazebo/Yellow</material>
  </gazebo>
</robot>
